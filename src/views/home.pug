    script.
      function showTab(tabId) {
        // Hide all tab contents
        document.querySelectorAll('.tab-content > div').forEach(function(content) {
          content.classList.remove('active');
        });

        // Remove active class from all nav links
        document.querySelectorAll('.navbar-nav .nav-link').forEach(function(link) {
          link.classList.remove('active');
        });

        // Show the selected tab content and add active class to the corresponding link
        document.getElementById(tabId).classList.add('active');
        document.getElementById(tabId + 'Link').classList.add('active');
      }

      // Add event listeners to tabs
      document.getElementById('proLink').addEventListener('click', function(event) {
        event.preventDefault();
        showTab('proContent');
      });

      document.getElementById('gaLink').addEventListener('click', function(event) {
        event.preventDefault();
        showTab('gaContent');
      });

      // Initialize the first tab as active
      showTab('proContent');
