    style.
      .tab-content {
        position: relative;
        height: 200px; /* Adjust height as needed */
      }
      .tab-content > div {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        opacity: 0;
        visibility: hidden;
        transition: opacity 0.4s ease-in-out, visibility 0.4s ease-in-out;
      }
      .tab-content > .active {
        opacity: 1;
        visibility: visible;
      }
      .satisfaction-text {
        color: #4c9ac7; /* Muted blue color */
        font-weight: bold;
        text-shadow: 0 0 2px #4c9ac7, 0 0 4px #4c9ac7, 0 0 6px #4c9ac7;
        animation: glow 1.5s infinite alternate;
      }
      @keyframes glow {
        0% {
          text-shadow: 0 0 2px #4c9ac7, 0 0 4px #4c9ac7, 0 0 6px #4c9ac7;
        }
        100% {
          text-shadow: 0 0 4px #4c9ac7, 0 0 6px #4c9ac7, 0 0 8px #4c9ac7;
        }
      }
